  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) okcc",
      "type": "cppdbg",
      "request": "launch",
      "program": "/usr/bin/env",
      "args": ["-i", "HOME=${HOME:-/home/agreppin}",
        "${workspaceFolder}/okcc", "okt5.sh"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceRoot}",
      "environment": [ {
        "name": "LD_LIBRARY_PATH",
        "value": "${workspaceFolder}"
      }],
      "externalConsole": false,
      "MIMode": "gdb",
    },
    {
      "name": "(gdb) oksh",
